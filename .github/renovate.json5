{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "schedule:monthly",
    "group:allNonMajor",
    "group:monorepos",
    ":semanticPrefixFixDepsChoreOthers",
    ":automergePatch",
    ":disableDependencyDashboard"
  ],
  "labels": ["dependencies"],
  "rangeStrategy": "bump",
  "postUpdateOptions": [
    "npmDedupe"
  ]
}
